}

info_tab {
  border: 1px solid #1d5994;
  background-color: rgba(0, 0, 75, 185);
}

units_reports QScrollArea {
  background: rgba(0, 0, 0, 45);
}

units_reports QScrollArea QWidget {
  background: rgba(0, 0, 0, 45);
}

unittype_item {
  border: none;
  background-color: rgba(0, 0, 0, 95);
  selection-background-color: rgba(50, 150, 250, 220);
}

units_reports {
  border: 1px solid #1d5994;
  background-color: rgba(0, 0, 75, 95);
}

QTextEdit#scenarios_view {
  color: white;
  background-color: #3d4552;
}

QTextEdit {
  background-color: #232323;
  color: #54ff54;
}

QFontListView {
  color: white;
  background-color: #3d4552;
}

QScrollBar:vertical {
  border: 1px solid #246db6;
}

QScrollBar:horizontal {
  border: 1px solid #246db6;
}

QScrollBar::sub-line:vertical {
  width: 0px;
  height: 0px;
}

QScrollBar::sub-page:vertical {
  background: #232323;
  width: 0px;
  height: 0px;
}

QScrollBar::add-line:vertical {
  width: 0px;
  height: 0px;
}

QScrollBar::add-page:vertical {
  background: #232323;
  width: 0px;
  height: 0px;
}

QScrollBar::handle:vertical {
